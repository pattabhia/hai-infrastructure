# HAI Intel Keycloak Infrastructure Environment Variables

# PostgreSQL Configuration
POSTGRES_DB=keycloak
POSTGRES_USER=keycloak
POSTGRES_PASSWORD=keycloak_password

# Keycloak Configuration
KEYCLOAK_ADMIN=admin
KEYCLOAK_ADMIN_PASSWORD=admin
KC_HOSTNAME=localhost
KC_HOSTNAME_PORT=8080
KC_DB=postgres
KC_DB_URL=jdbc:postgresql://haiintel-keycloak-postgres:5432/keycloak
KC_DB_USERNAME=keycloak
KC_DB_PASSWORD=keycloak_password
KC_LOG_LEVEL=INFO

# PostgreSQL Exporter Configuration
DATA_SOURCE_NAME=postgresql://keycloak:keycloak_password@haiintel-keycloak-postgres:5432/keycloak?sslmode=disable

# Network (must match shared-infra network)
NETWORK_NAME=haiintel-network

