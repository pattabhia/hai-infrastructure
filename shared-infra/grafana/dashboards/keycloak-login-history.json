          }
        ]
      },
      "pluginVersion": "10.2.2",
      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT \n  u.username,\n  u.email,\n  u.enabled,\n  TO_TIMESTAMP(u.created_timestamp / 1000) as created_at,\n  TO_TIMESTAMP(MAX(e.event_time) / 1000) as last_login,\n  COUNT(CASE WHEN e.type = 'LOGIN' THEN 1 END) as login_count,\n  COUNT(CASE WHEN e.type = 'LOGIN_ERROR' THEN 1 END) as failed_login_count\nFROM user_entity u\nLEFT JOIN event_entity e ON u.id = e.user_id AND e.type IN ('LOGIN', 'LOGIN_ERROR')\nWHERE u.realm_id = (SELECT id FROM realm WHERE name = 'haiintel')\nGROUP BY u.id, u.username, u.email, u.enabled, u.created_timestamp\nORDER BY last_login DESC NULLS LAST",
          "refId": "A"
        }
      ],
      "title": "User Login Summary",
      "type": "table"
    },
