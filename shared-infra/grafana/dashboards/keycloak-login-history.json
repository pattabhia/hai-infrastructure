      "targets": [
        {
          "format": "table",
          "rawSql": "SELECT \n  COALESCE(u.username, 'Unknown') as username,\n  COALESCE(u.email, 'N/A') as email,\n  e.ip_address,\n  COUNT(*) as failed_attempts,\n  MAX(TO_TIMESTAMP(e.event_time / 1000)) as last_failed_attempt\nFROM event_entity e\nLEFT JOIN user_entity u ON e.user_id = u.id\nWHERE e.realm_id = (SELECT id FROM realm WHERE name = 'haiintel')\n  AND e.type = 'LOGIN_ERROR'\n  AND e.event_time > EXTRACT(EPOCH FROM NOW() - INTERVAL '7 days') * 1000\nGROUP BY u.username, u.email, e.ip_address\nORDER BY failed_attempts DESC\nLIMIT 50",
          "refId": "A"
        }
      ],
      "title": "Failed Login Attempts (Last 7 Days)",
